{
    "index.html": {
        "title": "Templates index",
        "header_link": "{{ HOSTNAME_PROTOCOL }}",
        "download_text_1": "{% trans 'This email was sent by Flyers Concierge App' %}.",
        "download_text_2": "{% trans 'Download the App for free!' %}",
        "download_link_1": "https://itunes.apple.com/es/app/flyers-concierge/id1035360024?mt=8",
        "download_img_link_1": "static/emails/images/market_badges/apple_market_{{ LANGUAGE_CODE }}.png",
        "download_link_2": "https://play.google.com/store/apps/details?id=com.flyersconcierge.tracker&amp;hl=es",
        "download_img_link_2": "static/emails/images/market_badges/android_market_{{ LANGUAGE_CODE }}.png",
        "banner_include": "{{ banner_html | safe }}"
    },
    "user_order.html": {
        "title": "User order",
        "header_title": "{% trans 'Welcome to Flyers Concierge!' %}",
        "header_link": "https://www.flyersconcierge.com/",
        "banner_include": "<img src='static/emails/images/banner.png'>",
        "text_before_button": "{% trans 'Thank you for requesting the download link for the Flyers Concierge App. Just open this email on your cell phone or tablet and click on the button below.' %}",
        "download_text_1": "{% trans 'This email was sent by Flyers Concierge App' %}.",
        "download_text_2": "{% trans 'Download the App for free!' %}",
        "download_link_1": "https://itunes.apple.com/es/app/flyers-concierge/id1035360024?mt=8",
        "download_img_link_1": "static/emails/images/market_badges/apple_market_{{ LANGUAGE_CODE }}.png",
        "download_link_2": "https://play.google.com/store/apps/details?id=com.flyersconcierge.tracker&amp;hl=es",
        "download_img_link_2": "static/emails/images/market_badges/android_market_{{ LANGUAGE_CODE }}.png",
        "order_status_text": "{% trans 'Order status:' %} {{ order.status | order_status_language }}.",
        "user": "{{ order.fc_user_name }}",
        "message": "{{ msg }}",
        "tenant_name": "{{ order.delivery.tenant.name }}",
        "tenant_airport_city": "{{ airport.city }}, {{ airport.country_code }}",
        "tenant_airport_name": "{{ airport.name }}",
        "tenant_airport_fs": "{{ airport.fs }}",
        "pickup_at": "{{ order.pickup_at }}",
        "time_email": "{{ time_email }}",
        "pickup_place": "{{ sector.terminal }}, {{ sector.nivel }} {% trans 'level' %}, {{ sector.sector }}"
    }
}